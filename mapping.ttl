@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .

@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix : <http://ldf.fi/ammo/> .

@base <http://example.com/base/> .

<TriplesMap1> a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "data/ammo.csv";
    rml:referenceFormulation ql:CSV
  ];

  rr:subjectMap [ rr:template "http://ldf.fi/ammo/{uri}"; rr:termType rr:IRI; ];

  rr:predicateObjectMap [ 
    rr:predicate rdf:type; 
    rr:object :Occupation ;
  ];

  rr:predicateObjectMap [ 
    rr:predicate skos:prefLabel; 
    rr:objectMap [ rml:reference "main_name" ]
  ];

  rr:predicateObjectMap [ 
    rr:predicate skos:altLabel; 
    rr:objectMap [ rml:reference "secondary_name" ]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :coo1980_code; 
    rr:objectMap [ rr:template "http://ldf.fi/coo1980/{111}"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :hisco_code; 
    rr:objectMap [ rr:template "http://ldf.fi/hisco/{HISCO}"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :hisco_relation; 
    rr:objectMap [ rr:template "http://ldf.fi/hisco/relation/{RELATION}"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :hisco_status; 
    rr:objectMap [ rr:template "http://ldf.fi/hisco/status/{STATUS}"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :hisco_product; 
    rr:objectMap [ rr:template "http://ldf.fi/hisco/product/{PRODUCT}"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :hisclass; 
    rr:objectMap [ rr:template "http://ldf.fi/hisco/hisclass/{HISCLASS 2}"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :related; 
    rr:objectMap [ rml:reference "KOKO_concept"]
  ];

  rr:predicateObjectMap [ 
    rr:predicate :related; 
    rr:objectMap [
      rr:parentTriplesMap <TriplesMap2> ;
      rr:joinCondition [
        rr:child "onko_arvo O oppiarvo A arvonimi S sotilasarvo R rooli Sosioekonominen SE" ;
        rr:parent "abbreviation" ;
      ];
    ];
  ];

  .

<TriplesMap2> a rr:TriplesMap;
    
  rml:logicalSource [
    rml:source "input/classes.csv";
    rml:referenceFormulation ql:CSV ;
  ];

  rr:subjectMap [ rml:reference "uri" ];

  .


